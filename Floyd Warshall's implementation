#include <iostream>
#include <climits>
using namespace std;

int main() {
    int n;
    std::cin>>n;
    int r;
    std::cin >> r;
    int dist[n+10][n+10];
    for(int i=0;i<n;i++){
        for(int j=0;j<n;j++){
            dist[i][j] = INT_MAX;
        }
    }
    for(int i=0;i<n;i++){
        dist[i][i]=0;
    }
    for(int i=0;i<r;i++){
        int a,b,c;
        std::cin>>a>>b>>c;
        dist[a][b]=c;
    }
	for(int i=0;i<n;i++){
	    for(int j=0;j<n;j++){
	        for(int k=0;k<n;k++){
	            if(dist[i][j]+dist[j][k] < dist[i][k]){
	                dist[i][k] = dist[i][j]+dist[j][k];
	            }
	        }
	    }
	}
	
}
